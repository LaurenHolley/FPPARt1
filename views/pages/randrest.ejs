<!DOCTYPE html>
<html lang="en">
<head>
    <%- include('../partials/head'); %>
</head>
<body class="container">

<header>
    <%- include('../partials/header'); %>
</header>

<main>
    <h1>Random Restaurant Selection!</h1>
    <p>Please select users that will be present at your dinner party!</p>
    <ul>
        <% if (locals.users) { %>

            <% locals.users.forEach(function(user) { %>
                <li>
                    <input type="checkbox"> <%= user.first_name %>
                    
                </li>
            <% }); %>
           
        <% } %>
        
    </ul>

</main>

<footer>
    <%- include('../partials/footer'); %>
</footer>

</body>
</html>
